@PACKAGE_INIT@

# Include helper macro for finding dependencies
include(CMakeFindDependencyMacro)

# Find dependencies required by the public interface of memilio
# Adjust versions and components to match your project's requirements
find_dependency(Eigen3 3.4 REQUIRED)         # Replace 3.4 with your minimum Eigen version
find_dependency(Boost 1.76 REQUIRED COMPONENTS filesystem) # Replace 1.70 and add any other PUBLIC Boost components
find_dependency(Random123 REQUIRED)       # If Random123 is found via find_package

get_filename_component(MEMILIO_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

if(NOT TARGET @PROJECT_NAME@::@PROJECT_NAME@)
    include("${MEMILIO_CMAKE_DIR}/@PROJECT_NAME@-targets.cmake")
endif()